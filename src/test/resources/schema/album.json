                "default": "",
                "examples": [
                  "6OmhkSOpvYBokMKQxpIGx2"
                ],
                "pattern": "^(.*)$"
              },
              "is_local": {
                "$id": "#/properties/tracks/properties/items/items/properties/is_local",
                "type": "boolean",
                "title": "The Is_local Schema",
                "default": false,
                "examples": [
                  false
                ]
              },
              "name": {
                "$id": "#/properties/tracks/properties/items/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Global Warming"
                ],
                "pattern": "^(.*)$"
