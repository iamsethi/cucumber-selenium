                    "LT",
                    "LU",
                    "LV",
                    "MA",
                    "MC",
                    "MT",
                    "MX",
                    "NL",
                    "NO",
                    "OM",
                    "PL",
                    "PS",
                    "PT",
                    "QA",
                    "RO",
                    "SA",
                    "SE",
                    "SK",
                    "TN",
                    "TR",
                    "ZA"
                  ],
                  "pattern": "^(.*)$"
                }
              },
              "disc_number": {
                "$id": "#/properties/tracks/properties/items/items/properties/disc_number",
                "type": "integer",
                "title": "The Disc_number Schema",
                "default": 0,
                "examples": [
                  1
                ]
              },
              "duration_ms": {
                "$id": "#/properties/tracks/properties/items/items/properties/duration_ms",
                "type": "integer",
                "title": "The Duration_ms Schema",
                "default": 0,
                "examples": [
                  85400
                ]
              },
              "explicit": {
                "$id": "#/properties/tracks/properties/items/items/properties/explicit",
                "type": "boolean",
                "title": "The Explicit Schema",
                "default": false,
                "examples": [
                  true
                ]
              },
              "external_urls": {
                "$id": "#/properties/tracks/properties/items/items/properties/external_urls",
                "type": "object",
                "title": "The External_urls Schema",
                "required": [
                  "spotify"
                ],
                "properties": {
                  "spotify": {
                    "$id": "#/properties/tracks/properties/items/items/properties/external_urls/properties/spotify",
                    "type": "string",
                    "title": "The Spotify Schema",
                    "default": "",
                    "examples": [
                      "https://open.spotify.com/track/6OmhkSOpvYBokMKQxpIGx2"
