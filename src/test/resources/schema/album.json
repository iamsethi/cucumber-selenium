                ],
                "pattern": "^(.*)$"
              },
              "track_number": {
                "$id": "#/properties/tracks/properties/items/items/properties/track_number",
                "type": "integer",
                "title": "The Track_number Schema",
                "default": 0,
                "examples": [
                  1
                ]
              },
              "type": {
                "$id": "#/properties/tracks/properties/items/items/properties/type",
                "type": "string",
                "title": "The Type Schema",
                "default": "",
                "examples": [
                  "track"
                ],
                "pattern": "^(.*)$"
              },
              "uri": {
                "$id": "#/properties/tracks/properties/items/items/properties/uri",
                "type": "string",
                "title": "The Uri Schema",
                "default": "",
                "examples": [
                  "spotify:track:6OmhkSOpvYBokMKQxpIGx2"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "limit": {
          "$id": "#/properties/tracks/properties/limit",
          "type": "integer",
          "title": "The Limit Schema",
          "default": 0,
          "examples": [
            50
          ]
        },
        "next": {
          "$id": "#/properties/tracks/properties/next",
          "type": "null",
          "title": "The Next Schema",
          "default": null,
          "examples": [
            null
          ]
        },
        "offset": {
          "$id": "#/properties/tracks/properties/offset",
          "type": "integer",
          "title": "The Offset Schema",
          "default": 0,
          "examples": [
            0
          ]
        },
        "previous": {
          "$id": "#/properties/tracks/properties/previous",
          "type": "null",
          "title": "The Previous Schema",
          "default": null,
          "examples": [
            null
          ]
        },
        "total": {
          "$id": "#/properties/tracks/properties/total",
          "type": "integer",
          "title": "The Total Schema",
          "default": 0,
          "examples": [
            18
          ]
        }
      }
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "album"
      ],
      "pattern": "^(.*)$"
    },
    "uri": {
      "$id": "#/properties/uri",
      "type": "string",
      "title": "The Uri Schema",
      "default": "",
      "examples": [
        "spotify:album:4aawyAB9vmqN3uQ7FjRGTy"
