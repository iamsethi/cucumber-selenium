{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "album_type",
    "artists",
    "available_markets",
    "copyrights",
    "external_ids",
    "external_urls",
    "genres",
    "href",
    "id",
    "images",
    "label",
    "name",
    "popularity",
    "release_date",
    "release_date_precision",
    "total_tracks",
    "tracks",
    "type",
    "uri"
  ],
  "properties": {
    "album_type": {
      "$id": "#/properties/album_type",
      "type": "string",
      "title": "The Album_type Schema",
      "default": "",
      "examples": [
        "album"
      ],
      "pattern": "^(.*)$"
    },
    "artists": {
      "$id": "#/properties/artists",
      "type": "array",
      "title": "The Artists Schema",
      "items": {
        "$id": "#/properties/artists/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "external_urls",
          "href",
          "id",
          "name",
          "type",
          "uri"
        ],
        "properties": {
          "external_urls": {
            "$id": "#/properties/artists/items/properties/external_urls",
            "type": "object",
            "title": "The External_urls Schema",
            "required": [
              "spotify"
            ],
            "properties": {
              "spotify": {
                "$id": "#/properties/artists/items/properties/external_urls/properties/spotify",
                "type": "string",
                "title": "The Spotify Schema",
                "default": "",
                "examples": [
                  "https://open.spotify.com/artist/0TnOYISbd1XYRBk9myaseg"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "href": {
            "$id": "#/properties/artists/items/properties/href",
            "type": "string",
            "title": "The Href Schema",
            "default": "",
            "examples": [
              "https://api.spotify.com/v1/artists/0TnOYISbd1XYRBk9myaseg"
            ],
            "pattern": "^(.*)$"
          },
          "id": {
            "$id": "#/properties/artists/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "0TnOYISbd1XYRBk9myaseg"
            ],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/artists/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "Pitbull"
            ],
            "pattern": "^(.*)$"
          },
          "type": {
            "$id": "#/properties/artists/items/properties/type",
            "type": "string",
            "title": "The Type Schema",
            "default": "",
            "examples": [
              "artist"
            ],
            "pattern": "^(.*)$"
          },
          "uri": {
            "$id": "#/properties/artists/items/properties/uri",
            "type": "string",
            "title": "The Uri Schema",
            "default": "",
            "examples": [
              "spotify:artist:0TnOYISbd1XYRBk9myaseg"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "available_markets": {
      "$id": "#/properties/available_markets",
      "type": "array",
      "title": "The Available_markets Schema",
      "items": {
        "$id": "#/properties/available_markets/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "AD",
          "AE",
          "AT",
          "BE",
          "BG",
          "BH",
          "CH",
          "CY",
          "CZ",
          "DE",
          "DZ",
          "EE",
          "EG",
          "ES",
          "FI",
          "FR",
          "GB",
          "GR",
          "HU",
          "IE",
          "IL",
          "IT",
          "JO",
          "KW",
          "LB",
          "LI",
          "LT",
          "LU",
          "LV",
          "MA",
          "MC",
          "MT",
          "MX",
          "NL",
          "NO",
          "OM",
          "PL",
          "PS",
          "PT",
          "QA",
          "RO",
          "SA",
          "SE",
          "SK",
          "TN",
          "TR",
          "ZA"
        ],
        "pattern": "^(.*)$"
      }
    },
    "copyrights": {
      "$id": "#/properties/copyrights",
      "type": "array",
      "title": "The Copyrights Schema",
      "items": {
        "$id": "#/properties/copyrights/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "text",
          "type"
        ],
        "properties": {
          "text": {
            "$id": "#/properties/copyrights/items/properties/text",
            "type": "string",
            "title": "The Text Schema",
            "default": "",
            "examples": [
              "(P) 2012 RCA Records, a division of Sony Music Entertainment"
            ],
            "pattern": "^(.*)$"
          },
          "type": {
            "$id": "#/properties/copyrights/items/properties/type",
            "type": "string",
            "title": "The Type Schema",
            "default": "",
            "examples": [
              "P"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "external_ids": {
      "$id": "#/properties/external_ids",
      "type": "object",
      "title": "The External_ids Schema",
      "required": [
        "upc"
      ],
      "properties": {
        "upc": {
          "$id": "#/properties/external_ids/properties/upc",
          "type": "string",
          "title": "The Upc Schema",
          "default": "",
          "examples": [
            "886443671584"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "external_urls": {
      "$id": "#/properties/external_urls",
      "type": "object",
      "title": "The External_urls Schema",
      "required": [
        "spotify"
      ],
      "properties": {
        "spotify": {
          "$id": "#/properties/external_urls/properties/spotify",
          "type": "string",
          "title": "The Spotify Schema",
          "default": "",
          "examples": [
            "https://open.spotify.com/album/4aawyAB9vmqN3uQ7FjRGTy"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "genres": {
      "$id": "#/properties/genres",
      "type": "array",
      "title": "The Genres Schema"
    },
    "href": {
      "$id": "#/properties/href",
      "type": "string",
      "title": "The Href Schema",
      "default": "",
      "examples": [
        "https://api.spotify.com/v1/albums/4aawyAB9vmqN3uQ7FjRGTy"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "4aawyAB9vmqN3uQ7FjRGTy"
      ],
      "pattern": "^(.*)$"
    },
    "images": {
      "$id": "#/properties/images",
      "type": "array",
      "title": "The Images Schema",
      "items": {
        "$id": "#/properties/images/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "height",
          "url",
          "width"
        ],
        "properties": {
          "height": {
            "$id": "#/properties/images/items/properties/height",
            "type": "integer",
            "title": "The Height Schema",
            "default": 0,
            "examples": [
              640
            ]
          },
          "url": {
            "$id": "#/properties/images/items/properties/url",
            "type": "string",
            "title": "The Url Schema",
            "default": "",
            "examples": [
              "https://i.scdn.co/image/3edb3f970f4a3af9ef922efd18cdb4dabaf85ced"
            ],
            "pattern": "^(.*)$"
          },
          "width": {
            "$id": "#/properties/images/items/properties/width",
            "type": "integer",
            "title": "The Width Schema",
            "default": 0,
            "examples": [
              640
            ]
          }
        }
      }
    },
    "label": {
      "$id": "#/properties/label",
      "type": "string",
      "title": "The Label Schema",
      "default": "",
      "examples": [
        "Mr.305/Polo Grounds Music/RCA Records"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Global Warming"
      ],
      "pattern": "^(.*)$"
    },
    "popularity": {
      "$id": "#/properties/popularity",
      "type": "integer",
      "title": "The Popularity Schema",
      "default": 0,
      "examples": [
        55
      ]
    },
    "release_date": {
      "$id": "#/properties/release_date",
      "type": "string",
      "title": "The Release_date Schema",
      "default": "",
      "examples": [
        "2012-11-16"
      ],
      "pattern": "^(.*)$"
    },
    "release_date_precision": {
      "$id": "#/properties/release_date_precision",
      "type": "string",
      "title": "The Release_date_precision Schema",
      "default": "",
      "examples": [
        "day"
      ],
      "pattern": "^(.*)$"
    },
    "total_tracks": {
      "$id": "#/properties/total_tracks",
      "type": "integer",
      "title": "The Total_tracks Schema",
      "default": 0,
      "examples": [
        18
      ]
    },
    "tracks": {
      "$id": "#/properties/tracks",
      "type": "object",
      "title": "The Tracks Schema",
      "required": [
        "href",
        "items",
        "limit",
        "next",
        "offset",
        "previous",
        "total"
      ],
      "properties": {
        "href": {
          "$id": "#/properties/tracks/properties/href",
          "type": "string",
          "title": "The Href Schema",
          "default": "",
          "examples": [
            "https://api.spotify.com/v1/albums/4aawyAB9vmqN3uQ7FjRGTy/tracks?offset=0&limit=50"
          ],
          "pattern": "^(.*)$"
        },
        "items": {
          "$id": "#/properties/tracks/properties/items",
          "type": "array",
          "title": "The Items Schema",
          "items": {
            "$id": "#/properties/tracks/properties/items/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "artists",
              "available_markets",
              "disc_number",
              "duration_ms",
              "explicit",
              "external_urls",
              "href",
              "id",
              "is_local",
              "name",
              "preview_url",
              "track_number",
              "type",
              "uri"
            ],
            "properties": {
              "artists": {
                "$id": "#/properties/tracks/properties/items/items/properties/artists",
                "type": "array",
                "title": "The Artists Schema",
                "items": {
                  "$id": "#/properties/tracks/properties/items/items/properties/artists/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "external_urls",
                    "href",
                    "id",
                    "name",
                    "type",
                    "uri"
                  ],
                  "properties": {
                    "external_urls": {
                      "$id": "#/properties/tracks/properties/items/items/properties/artists/items/properties/external_urls",
                      "type": "object",
                      "title": "The External_urls Schema",
                      "required": [
                        "spotify"
                      ],
                      "properties": {
                        "spotify": {
                          "$id": "#/properties/tracks/properties/items/items/properties/artists/items/properties/external_urls/properties/spotify",
