                          "pattern": "^(.*)$"
                        }
                      }
                    },
                    "href": {
                      "$id": "#/properties/tracks/properties/items/items/properties/artists/items/properties/href",
                      "type": "string",
                      "title": "The Href Schema",
                      "default": "",
                      "examples": [
                        "https://api.spotify.com/v1/artists/0TnOYISbd1XYRBk9myaseg"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "id": {
                      "$id": "#/properties/tracks/properties/items/items/properties/artists/items/properties/id",
                      "type": "string",
                      "title": "The Id Schema",
                      "default": "",
                      "examples": [
                        "0TnOYISbd1XYRBk9myaseg"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "name": {
                      "$id": "#/properties/tracks/properties/items/items/properties/artists/items/properties/name",
                      "type": "string",
                      "title": "The Name Schema",
                      "default": "",
                      "examples": [
                        "Pitbull"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "type": {
                      "$id": "#/properties/tracks/properties/items/items/properties/artists/items/properties/type",
                      "type": "string",
                      "title": "The Type Schema",
                      "default": "",
                      "examples": [
                        "artist"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "uri": {
                      "$id": "#/properties/tracks/properties/items/items/properties/artists/items/properties/uri",
                      "type": "string",
                      "title": "The Uri Schema",
                      "default": "",
                      "examples": [
                        "spotify:artist:0TnOYISbd1XYRBk9myaseg"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              },
              "available_markets": {
                "$id": "#/properties/tracks/properties/items/items/properties/available_markets",
                "type": "array",
                "title": "The Available_markets Schema",
                "items": {
                  "$id": "#/properties/tracks/properties/items/items/properties/available_markets/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "AD",
                    "AE",
                    "AT",
                    "BE",
                    "BG",
                    "BH",
                    "CH",
                    "CY",
                    "CZ",
                    "DE",
                    "DZ",
                    "EE",
                    "EG",
                    "ES",
                    "FI",
                    "FR",
                    "GB",
                    "GR",
                    "HU",
