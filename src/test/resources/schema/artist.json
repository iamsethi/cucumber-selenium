{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "href",
    "items",
    "limit",
    "next",
    "offset",
    "previous",
    "total"
  ],
  "properties": {
    "href": {
      "$id": "#/properties/href",
      "type": "string",
      "title": "The Href Schema",
      "default": "",
      "examples": [
        "https://api.spotify.com/v1/artists/0TnOYISbd1XYRBk9myaseg/albums?offset=0&limit=20&include_groups=album,single,compilation,appears_on"
      ],
      "pattern": "^(.*)$"
    },
    "items": {
      "$id": "#/properties/items",
      "type": "array",
      "title": "The Items Schema",
      "items": {
        "$id": "#/properties/items/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "album_group",
          "album_type",
          "artists",
          "available_markets",
          "external_urls",
          "href",
          "id",
          "images",
          "name",
          "release_date",
          "release_date_precision",
          "total_tracks",
          "type",
          "uri"
        ],
        "properties": {
          "album_group": {
            "$id": "#/properties/items/items/properties/album_group",
            "type": "string",
            "title": "The Album_group Schema",
            "default": "",
            "examples": [
              "album"
            ],
            "pattern": "^(.*)$"
          },
          "album_type": {
            "$id": "#/properties/items/items/properties/album_type",
            "type": "string",
            "title": "The Album_type Schema",
            "default": "",
            "examples": [
              "album"
            ],
            "pattern": "^(.*)$"
          },
          "artists": {
            "$id": "#/properties/items/items/properties/artists",
            "type": "array",
            "title": "The Artists Schema",
            "items": {
              "$id": "#/properties/items/items/properties/artists/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "external_urls",
                "href",
                "id",
                "name",
                "type",
                "uri"
              ],
              "properties": {
                "external_urls": {
                  "$id": "#/properties/items/items/properties/artists/items/properties/external_urls",
                  "type": "object",
                  "title": "The External_urls Schema",
                  "required": [
                    "spotify"
                  ],
                  "properties": {
                    "spotify": {
                      "$id": "#/properties/items/items/properties/artists/items/properties/external_urls/properties/spotify",
                      "type": "string",
                      "title": "The Spotify Schema",
                      "default": "",
                      "examples": [
                        "https://open.spotify.com/artist/0TnOYISbd1XYRBk9myaseg"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                },
                "href": {
                  "$id": "#/properties/items/items/properties/artists/items/properties/href",
                  "type": "string",
                  "title": "The Href Schema",
                  "default": "",
                  "examples": [
                    "https://api.spotify.com/v1/artists/0TnOYISbd1XYRBk9myaseg"
                  ],
                  "pattern": "^(.*)$"
                },
                "id": {
                  "$id": "#/properties/items/items/properties/artists/items/properties/id",
                  "type": "string",
                  "title": "The Id Schema",
                  "default": "",
                  "examples": [
                    "0TnOYISbd1XYRBk9myaseg"
                  ],
                  "pattern": "^(.*)$"
                },
                "name": {
                  "$id": "#/properties/items/items/properties/artists/items/properties/name",
                  "type": "string",
                  "title": "The Name Schema",
                  "default": "",
                  "examples": [
                    "Pitbull"
                  ],
                  "pattern": "^(.*)$"
                },
                "type": {
                  "$id": "#/properties/items/items/properties/artists/items/properties/type",
                  "type": "string",
                  "title": "The Type Schema",
                  "default": "",
                  "examples": [
                    "artist"
                  ],
                  "pattern": "^(.*)$"
                },
                "uri": {
                  "$id": "#/properties/items/items/properties/artists/items/properties/uri",
                  "type": "string",
                  "title": "The Uri Schema",
                  "default": "",
                  "examples": [
                    "spotify:artist:0TnOYISbd1XYRBk9myaseg"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          },
          "available_markets": {
            "$id": "#/properties/items/items/properties/available_markets",
            "type": "array",
            "title": "The Available_markets Schema",
            "items": {
              "$id": "#/properties/items/items/properties/available_markets/items",
              "type": "string",
              "title": "The Items Schema",
              "default": "",
              "examples": [
                "AD",
                "AE",
                "AR",
                "AT",
                "AU",
                "BE",
                "BG",
                "BH",
                "BO",
                "BR",
