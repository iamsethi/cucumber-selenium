                "NZ",
                "OM",
                "PA",
                "PE",
                "PH",
                "PL",
                "PS",
                "PT",
                "PY",
                "QA",
                "RO",
                "SA",
                "SE",
                "SG",
                "SK",
                "SV",
                "TH",
                "TN",
                "TR",
                "TW",
                "US",
                "UY",
                "VN",
                "ZA"
              ],
              "pattern": "^(.*)$"
            }
          },
          "external_urls": {
            "$id": "#/properties/items/items/properties/external_urls",
            "type": "object",
            "title": "The External_urls Schema",
            "required": [
              "spotify"
            ],
            "properties": {
              "spotify": {
                "$id": "#/properties/items/items/properties/external_urls/properties/spotify",
                "type": "string",
                "title": "The Spotify Schema",
                "default": "",
                "examples": [
                  "https://open.spotify.com/album/6ZSNnOY2ESMNoVQ5DdvHrj"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "href": {
            "$id": "#/properties/items/items/properties/href",
            "type": "string",
            "title": "The Href Schema",
            "default": "",
            "examples": [
              "https://api.spotify.com/v1/albums/6ZSNnOY2ESMNoVQ5DdvHrj"
            ],
            "pattern": "^(.*)$"
          },
          "id": {
            "$id": "#/properties/items/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "6ZSNnOY2ESMNoVQ5DdvHrj"
            ],
            "pattern": "^(.*)$"
          },
          "images": {
            "$id": "#/properties/items/items/properties/images",
            "type": "array",
            "title": "The Images Schema",
            "items": {
              "$id": "#/properties/items/items/properties/images/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "height",
                "url",
                "width"
              ],
              "properties": {
                "height": {
                  "$id": "#/properties/items/items/properties/images/items/properties/height",
                  "type": "integer",
                  "title": "The Height Schema",
                  "default": 0,
                  "examples": [
                    640
                  ]
                },
                "url": {
                  "$id": "#/properties/items/items/properties/images/items/properties/url",
                  "type": "string",
                  "title": "The Url Schema",
                  "default": "",
                  "examples": [
                    "https://i.scdn.co/image/8544de8a2b35dc3a96b80df492919314ba1a9e30"
                  ],
                  "pattern": "^(.*)$"
                },
                "width": {
                  "$id": "#/properties/items/items/properties/images/items/properties/width",
                  "type": "integer",
                  "title": "The Width Schema",
                  "default": 0,
                  "examples": [
                    640
                  ]
                }
              }
            }
          },
          "name": {
            "$id": "#/properties/items/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "Gotti (Original Motion Picture Soundtrack)"
            ],
            "pattern": "^(.*)$"
          },
          "release_date": {
            "$id": "#/properties/items/items/properties/release_date",
            "type": "string",
            "title": "The Release_date Schema",
            "default": "",
            "examples": [
              "2018-06-14"
            ],
            "pattern": "^(.*)$"
          },
          "release_date_precision": {
            "$id": "#/properties/items/items/properties/release_date_precision",
            "type": "string",
            "title": "The Release_date_precision Schema",
            "default": "",
            "examples": [
              "day"
            ],
            "pattern": "^(.*)$"
          },
          "total_tracks": {
            "$id": "#/properties/items/items/properties/total_tracks",
            "type": "integer",
            "title": "The Total_tracks Schema",
            "default": 0,
            "examples": [
              19
            ]
          },
          "type": {
            "$id": "#/properties/items/items/properties/type",
            "type": "string",
            "title": "The Type Schema",
            "default": "",
            "examples": [
              "album"
            ],
            "pattern": "^(.*)$"
          },
          "uri": {
            "$id": "#/properties/items/items/properties/uri",
            "type": "string",
            "title": "The Uri Schema",
            "default": "",
            "examples": [
              "spotify:album:6ZSNnOY2ESMNoVQ5DdvHrj"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "limit": {
      "$id": "#/properties/limit",
      "type": "integer",
      "title": "The Limit Schema",
      "default": 0,
      "examples": [
        20
      ]
    },
    "next": {
      "$id": "#/properties/next",
      "type": "string",
      "title": "The Next Schema",
      "default": "",
      "examples": [
        "https://api.spotify.com/v1/artists/0TnOYISbd1XYRBk9myaseg/albums?offset=20&limit=20&include_groups=album,single,compilation,appears_on"
      ],
      "pattern": "^(.*)$"
    },
    "offset": {
      "$id": "#/properties/offset",
      "type": "integer",
      "title": "The Offset Schema",
      "default": 0,
      "examples": [
        0
      ]
    },
    "previous": {
      "$id": "#/properties/previous",
      "type": "null",
      "title": "The Previous Schema",
      "default": null,
      "examples": [
        null
      ]
    },
    "total": {
      "$id": "#/properties/total",
      "type": "integer",
      "title": "The Total Schema",
      "default": 0,
      "examples": [
        534
      ]
    }
  }
